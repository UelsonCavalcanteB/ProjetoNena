{%CLASSGROUP 'Vcl.Controls.TControl'}

{$R *.dfm}

procedure Tdm.ibEndClienteNewRecord(DataSet: TDataSet);
begin
  ibEndClienteEX.Value := '0';

end;

procedure Tdm.IB_ClienteNewRecord(DataSet: TDataSet);
begin
  IB_ClienteINATIVO.Value := 'N';
  IB_ClienteTIPO.Value    := 'F';
  IB_ClienteX.Value       := '0';
end;

procedure Tdm.Ib_EnderecoNewRecord(DataSet: TDataSet);
begin
  Ib_EnderecoEX.Value := '0';
end;

procedure Tdm.ib_fpgNewRecord(DataSet: TDataSet);
begin
  ib_fpgATIVO.Value := 'S';
  ib_fpgEX.Value    := '0';
end;

procedure Tdm.ib_FuncionarioNewRecord(DataSet: TDataSet);
begin
  ib_FuncionarioEX.Value := '0';
  ib_FuncionarioATIVO.Value := 'N';
end;

procedure Tdm.ib_grupoNewRecord(DataSet: TDataSet);
begin
  ib_grupoEX.Value := '0';
  ib_grupoATIVO.Value := 'S';
end;

procedure Tdm.ib_marcaNewRecord(DataSet: TDataSet);
begin
  ib_marcaATIVO.Value := 'S';
  ib_marcaEX.Value    := '0';
end;

